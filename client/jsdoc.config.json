{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src"],
    "includePattern": "\\.(jsx|js)$",
    "excludePattern": "(node_modules|build)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "destination": "../docs/frontend",
    "recurse": true,
    "readme": "README.md",
    "template": "node_modules/clean-jsdoc-theme"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": true,
      "useLongnameInNav": true,
      "navType": "vertical"
    }
  }
}