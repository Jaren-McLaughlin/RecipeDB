{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["."],
    "includePattern": ".js",
    "excludePattern": "node_modules"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "destination": "../docs/backend",
    "recurse": true,
    "readme": "README.md",
    "template": "node_modules/clean-jsdoc-theme"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": true,
      "useLongnameInNav": true,
      "navType": "vertical"
    }
  }
}